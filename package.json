{
  "name": "moody",
  "version": "1.0.1",
  "private": true,
  "description": "Create perfect playlists",
  "homepage": "http://localhost:8888",
  "keywords": [
    "moody",
    "spotify",
    "music",
    "playlist"
  ],
  "main": "index.js",
  "scripts": {
    "nodemon": "cross-env BABEL_ENV=server NODE_ENV=development nodemon --config nodemon.json",
    "webpack": "webpack-dev-server --mode development --hot --progress --color --open",
    "start": "npm run nodemon -- --exec 'node --inspect src/server' & npm run webpack",
    "test": "jest",
    "coverage": "jest --coverage",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "webpack -p --progress --colors"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@types/classnames": "^2.2.7",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.18",
    "@types/node": "^10.12.18",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/react-redux": "^7.0.0",
    "@types/react-router": "^4.4.3",
    "@types/redux-actions": "2.3.1",
    "@types/webpack": "^4.4.23",
    "@types/webpack-env": "1.13.6",
    "babel-loader": "^8.3.0",
    "clean-webpack-plugin": "^2.0.1",
    "cross-env": "^6.0.0",
    "css-loader": "^6.7.3",
    "enzyme": "^3.10.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^2.0.1",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^2.7.2",
    "nodemon": "^1.19.2",
    "postcss": "^7.0.13",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^6.5.0",
    "postcss-reporter": "^6.0.1",
    "postcss-url": "^8.0.0",
    "prettier": "^1.15.3",
    "react-hot-loader": "^4.13.1",
    "redux-devtools-extension": "^2.13.7",
    "style-loader": "^3.3.1",
    "ts-jest": "^24.1.0",
    "ts-loader": "^9.4.2",
    "typescript": "^3.2.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-middleware": "^3.7.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-hot-middleware": "^2.25.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.4",
    "dotenv": "^8.1.0",
    "express": "~4.16.0",
    "querystring": "~0.2.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "request": "~2.83.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(jpe?g|gif|bmp|mp3|mp4|ogg|wav|eot|ttf|woff|woff2)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "useTabs": false,
    "tabWidth": 2,
    "arrowParens": "always",
    "bracketSpacing": true,
    "trailingComma": "all",
    "jsxBracketSameLine": false,
    "printWidth": 100
  }
}
